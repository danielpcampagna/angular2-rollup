{
    "src": "src/{{projectName}}",
    "dist": "dist/{{projectName}}",
    "filename": "{{projectName}}",
    "es2015": {
        "tsConfig": "tsconfig.lib.json",
        "rollupConfig": "rollup.config.lib.js",
        "outFile": "dist/{{projectName}}/fesm2015/{{projectName}}.js"
    },
    "es5": {
        "tsConfig": "tsconfig.lib.es5.json",
        "rollupConfig": "rollup.config.lib-es5.js",
        "outFile": "dist/{{projectName}}/fesm5/{{projectName}}.js"
    },
    "umd": {
        "tsConfig": "tsconfig.lib.umd.json",
        "rollupConfig": "rollup.config.lib-umd.js",
        "outFile": "dist/{{projectName}}/bundles/{{projectName}}.umd.js"
    },
    "esm5": {
        "tsConfig": "tsconfig.lib.esm5.json",
        "outFile": "dist/{{projectName}}/esm5/{{projectName}}.js"
    },
    "esm2015": {
        "tsConfig": "tsconfig.lib.esm2015.json",
        "outFile": "dist/{{projectName}}/esm2015/{{projectName}}.js"
    }
}